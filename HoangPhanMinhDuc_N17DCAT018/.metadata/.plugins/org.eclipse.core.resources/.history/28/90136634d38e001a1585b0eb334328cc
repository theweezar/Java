package app.entity;

import java.util.*;
import javax.persistence.*;

@Entity
@Table(name="playlist_detail")
public class PlayListDetail {
	
	@Column(name="plId")
	private int plId;
	
	@Column(name="songId")
	private int songId;
	
	@Temporal(TemporalType.TIMESTAMP)
	@Column(name="add_at")
	private Date add_at;
	
	@OneToMany(mappedBy="id", fetch=FetchType.EAGER)
	private Collection<Song> songList;
}
