package ptithcm.entity;
import java.util.Date;

import javax.persistence.*;

@Entity
@Table(name="Records")
public class Records {

	@Id
	@GeneratedValue
	@Column(name="Id")
	private Integer id;
	@Column(name="Type")
	private Boolean type;
	@Column(name="Reason")
	private String reason;
	@Temporal(TemporalType.DATE)
	@Column(name="Date")
	private Date date;
	@Column(name="StaffId")
	private String staffId;
}
