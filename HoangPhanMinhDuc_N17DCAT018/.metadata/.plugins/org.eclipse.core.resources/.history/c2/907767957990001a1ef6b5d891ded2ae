package ptithcm.entity;

import java.util.Collection;

import javax.persistence.*;


@Entity
@Table(name="Departs")
public class Departs {
	@Id
	@Column(name="Id")
	private String id;
	@Column(name="Name")
	private String name;
	
	@OneToMany(mappedBy="depart",fetch=FetchType.EAGER)
	private Collection<Staffs> staffs;
}
